{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":ignoreModulesAndTests",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "ignorePaths": [
    "fixtures/"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["protoc-gen-*"]
    }
  ]
}
