      param {
        name: "enable_compile_jar_action"
        doc: "Enables header compilation or ijar creation. If set to False, it forces use of the full class jar in the compilation classpaths of any dependants. Doing so is intended for use by non-library targets such as binaries that do not have dependants."
        default_value: "True"
      }
      param {
        name: "add_exports"
        type: "<a class=\"anchor\" href=\"../core/list.html\">sequence</a> of <a class=\"anchor\" href=\"../core/string.html\">string</a>s"
        doc: "Allow this library to access the given <module>/<package>. Optional."
        default_value: "[]"
      }
      param {
        name: "add_opens"
        type: "<a class=\"anchor\" href=\"../core/list.html\">sequence</a> of <a class=\"anchor\" href=\"../core/string.html\">string</a>s"
        doc: "Allow this library to reflectively access the given <module>/<package>. Optional."
        default_value: "[]"
      }
      return_type: "struct"
    }
    doc: "Compiles Java source files/jars from the implementation of a Starlark rule and returns a provider that represents the results of the compilation and can be added to the set of providers emitted by this rule."
  }
  field {
