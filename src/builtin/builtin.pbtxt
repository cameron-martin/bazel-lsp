        name: "sources"
        type: "<a class=\"anchor\" href=\"../core/list.html\">sequence</a> of <a class=\"anchor\" href=\"../builtins/File.html\">File</a>s"
        doc: "A list of Java source files to be packed into the source jar."
        default_value: "[]"
      }
      param {
        name: "source_jars"
        type: "<a class=\"anchor\" href=\"../core/list.html\">sequence</a> of <a class=\"anchor\" href=\"../builtins/File.html\">File</a>s"
        doc: "A list of source jars to be packed into the source jar."
        default_value: "[]"
      }
      param {
        name: "java_toolchain"
        doc: "A JavaToolchainInfo to used to find the ijar tool."
