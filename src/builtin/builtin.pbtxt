  field {
    name: "template"
    callable {
      param {
        name: "path"
        type: "<a class=\"anchor\" href=\"../core/string.html\">string</a>; or <a class=\"anchor\" href=\"../builtins/Label.html\">Label</a>; or <a class=\"anchor\" href=\"../builtins/path.html\">path</a>"
        doc: "path of the file to create, relative to the repository directory."
        is_mandatory: true
      }
      param {
        name: "template"
        type: "<a class=\"anchor\" href=\"../core/string.html\">string</a>; or <a class=\"anchor\" href=\"../builtins/Label.html\">Label</a>; or <a class=\"anchor\" href=\"../builtins/path.html\">path</a>"
