      param {
        name: "end"
        type: "<a class=\"anchor\" href=\"../core/int.html\">int</a>; or <code>None</code>"
        doc: "optional position before which to restrict to search."
        default_value: "None"
      }
      return_type: "int"
    }
    doc: "Returns the first index where <code>sub</code> is found, or -1 if no such index exists, optionally restricting to <code>[start:end]</code>, <code>start</code> being inclusive and <code>end</code> being exclusive."
