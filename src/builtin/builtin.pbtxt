  }
  field {
    name: "endswith"
    callable {
      param {
        name: "sub"
        type: "<a class=\"anchor\" href=\"../core/string.html\">string</a>; or <a class=\"anchor\" href=\"../core/tuple.html\">tuple</a> of <a class=\"anchor\" href=\"../core/string.html\">string</a>s"
        doc: "The suffix (or tuple of alternative suffixes) to match."
        is_mandatory: true
      }
      param {
        name: "start"
        type: "<a class=\"anchor\" href=\"../core/int.html\">int</a>; or <code>None</code>"
